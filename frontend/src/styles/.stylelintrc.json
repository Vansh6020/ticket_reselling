{
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]+$",
      "declaration-empty-line-before": "never",
      "color-hex-length": "short",
      "at-rule-empty-line-before": "always",
      "media-feature-range-notation": "context"
    }
  }